{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
   "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/juegos/:tipoJuego/:oidJuego/:oidUsuario",
      "dest": "/juegos"
    },
    {
      "src": "/sopadeletras",
      "dest": "/sopadeletras"
    },
    {
      "src": "/ahorcado",
      "dest": "/ahorcado"
    },
    {
      "src": "/crucigrama",
      "dest": "/crucigrama"
    },
    {
      "src": "/trivia",
      "dest": "/trivia"
    }
  ]
}